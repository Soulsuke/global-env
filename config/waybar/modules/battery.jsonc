{
  /*\
  |* Battery
  \***************************************************************************/

  "battery": {
    "interval": 1,
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {time} {icon}",
    "format-icons": [ "", "", "", "", "" ],
    // Per status formats, see: `cat /sys/class/power_supply/BAT0/status`
    "format-charging": "{capacity}% ",
    "format-plugged": "",
    "format-full": ""
  }

}

