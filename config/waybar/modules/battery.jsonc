{
  /*\
  |* Battery
  \***************************************************************************/

  "battery": {
    "interval": 1,
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {time} {icon}",
    "format-full": "",
    "format-charging": "{capacity}% ",
    "format-plugged": "{capacity}% ",
    "format-alt": "{time} {icon}",
    "format-icons": [ "", "", "", "", "" ]
  }

}

