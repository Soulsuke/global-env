{
  /*\
  |* Hyprland
  \***************************************************************************/

  "hyprland/workspaces": {
    "show-special": true,
    "special-visible-only": true,
    "all-outputs": true,
    "on-click": "activate",
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "format":" {icon} ",
    "format-icons": {
      "1": "一",
      "2": "二",
      "3": "三",
      "4": "四",
      "5": "五",
      "6": "六",
      "7": "七",
      "8": "八",
      "9": "九",
      "10": "十",
      "floating": "",
      "terminal": ""
    }
  }

}

